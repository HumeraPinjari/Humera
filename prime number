      "source": [
        "import numpy as np\n",
        "from tensorflow import keras\n",
        "from tensorflow.keras import layers\n",
        "\n",
        "\n",
        "def generate_data(n_samples):\n",
        "    X = np.random.randint(2, 100, size=(n_samples, 1))\n",
        "    y = np.zeros((n_samples, 1))\n",
        "    for i, x in enumerate(X):\n",
        "        if is_prime(x[0]):\n",
        "            y[i] = 1\n",
        "    return X, y\n",
        "\n",
        "def is_prime(n):\n",
        "    if n <= 1:\n",
        "        return False\n",
        "    if n == 2:\n",
        "        return True\n",
        "    if n % 2 == 0:\n",
        "        return False\n",
        "    max_divisor = int(np.ceil(np.sqrt(n)))\n",
        "    for d in range(3, max_divisor + 1, 2):\n",
        "        if n % d == 0:\n",
        "            return False\n",
        "    return True\n",
        "\n",
        "\n",
        "X_train, y_train = generate_data(10000)\n",
        "X_test, y_test = generate_data(2000)\n",
        "\n",
        "\n",
        "model = keras.Sequential([\n",
        "    layers.Dense(64, activation='relu', input_shape=(1,)),\n",
        "    layers.Dense(64, activation='relu'),\n",
        "    layers.Dense(1, activation='sigmoid')\n",
        "])\n",
        "\n",
        "\n",
        "model.compile(optimizer='adam', loss='binary_crossentropy', metrics=['accuracy'])\n",
        "\n",
        "\n",
        "model.fit(X_train, y_train, epochs=10, batch_size=128, validation_data=(X_test, y_test))\n",
        "\n",
        "\n",
        "def predict_primes(n_samples):\n",
        "    X_pred = np.random.randint(2, 100, size=(n_samples, 1))\n",
        "    y_pred = model.predict(X_pred)\n",
        "    prime_numbers = X_pred[y_pred > 0.5]\n",
        "    return prime_numbers\n",
        "\n",
        "\n",
        "prime_numbers = predict_primes(100)\n",
        "print(prime_numbers)"
      ]
    }
  ]
}